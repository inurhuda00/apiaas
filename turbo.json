{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*", ".dev.vars"],
      "outputs": ["build/**", "public/build/**", "server/index.js"],
      "cache": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}